# {{ ansible_managed }}

{% if nginx_asset_extensions %}
# Directives to send expires headers and turn off 404 error logging for Static assets
location ~* ^.+\.({% for ext in nginx_asset_extensions %}{{ ext }}{% if loop.last == false %}|{% endif %}{% endfor %})$ {
    access_log off;
    log_not_found off;
    expires max;
    add_header Cache-Control public;
}
{% endif %}
